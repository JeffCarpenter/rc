keycode  37 = Hyper_L
keycode  64 = Control_L NoSymbol Control_L NoSymbol Control_L
keycode  66 = Alt_L Meta_L Alt_L Meta_L Alt_L Meta_L
keycode 108 = Control_R NoSymbol Control_R NoSymbol Control_R
keycode 105 = Alt_R Meta_R Alt_R Meta_R Alt_R Meta_R

clear Shift
clear Lock
clear Control
clear Mod1
clear Mod2
clear Mod3
clear Mod4
clear Mod5

add    Shift   = Shift_L Shift_R
add    Lock    = Caps_Lock
add    Control = Control_L Control_R
add    Mod1    = Alt_L Alt_R 0x00CD
add    Mod2    = Num_Lock
!add    Mod4    = 0x00CE 0x00CF
add    Mod3    = Hyper_L
add    Mod4    = Super_L
add    Mod5    = ISO_Level3_Shift Mode_switch
add    Lock    = Caps_Lock


