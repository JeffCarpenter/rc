
clear Shift
clear Lock
clear Control
clear Mod1
clear Mod2
clear Mod3
clear Mod4
clear Mod5

add    Shift   = Shift_L Shift_R
add    Lock    = Caps_Lock
add    Control = Control_L Control_R
add    Mod1    = Alt_L Alt_R 0x00CD
add    Mod2    = Num_Lock
add    Mod4    = 0x00CE 0x00CF
add    Mod5    = ISO_Level3_Shift Mode_switch
